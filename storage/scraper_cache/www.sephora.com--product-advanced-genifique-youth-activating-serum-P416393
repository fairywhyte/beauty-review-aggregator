        <script>
        (function(){
          var read_cookie = function (key) {
            var cookie_string = "" + document.cookie;
            var cookie_array = cookie_string.split("; ");
            for (var i = 0; i < cookie_array.length; ++i) {
              var single_cookie = cookie_array [i].split("=");
              if (single_cookie.length != 2)
                continue;
              var name = unescape(single_cookie[0]);
              var value = unescape(single_cookie[1]);
              // Return cookie if found:
              if (key == name)
                return value;
            }
            return null;
            };

            var cookieData, sessionData;
            var getUserData = function(field){
              if(!cookieData){
                cookieData = {};
              }

              if(!sessionData){
                var su = window.sessionStorage.getItem("sephUser");
                sessionData = su ? JSON.parse(su) : {};
              }

              userData = cookieData[field] ? cookieData : sessionData;

              return !userData[field] ? '' : userData[field];
            }
          })();
        </script>
